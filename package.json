    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-transition-group": "^4.0.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "sweetalert2": "^8.0.3",
    "sweetalert2-react-content": "^1.1.0"
  },
  "devDependencies": {
